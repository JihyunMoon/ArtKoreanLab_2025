{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "esModuleInterop": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
  "types": ["svelte"]
  },
  "include": ["src/**/*", "src/**/*.d.ts", "vite.config.ts"],
  "exclude": ["dist", "node_modules"]
}
